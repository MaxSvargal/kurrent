{
  "name": "kurrent-builder",
  "productName": "KurrentBuilder",
  "version": "0.1.0",
  "description": "p2p torrent tracker",
  "main": "main.js",
  "scripts": {
    "test": "NODE_ENV=test NODE_PATH=app nyc ava",
    "coverage": "nyc report --reporter=lcov --reporter=text-lcov | coveralls",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --retries 2 --compilers js:babel-register --require ./test/setup.js ./test/e2e.js",
    "lint": "eslint --ignore-path .gitignore --format=node_modules/eslint-formatter-pretty app test *.js",
    "lint-fix": "npm run lint -- --fix",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "flow-typed-install": "flow-typed install",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development",
    "postinstall": "concurrently \"npm run flow-typed-install\" \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "dev": "npm run hot-server -- --start-hot",
    "package": "npm run build && build --publish never",
    "package-win": "npm run build && build --win --x64",
    "package-linux": "npm run build && build --linux",
    "package-all": "npm run build && build -mwl"
  },
  "build": {
    "productName": "Kurrent",
    "appId": "com.electron.kurrent",
    "category": "public.app-category.network",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.js",
      "main.js.map",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    }
  },
  "directories": {
    "buildResources": "resources",
    "output": "release"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaxSvargal/kurrent.git"
  },
  "author": {
    "name": "MaxSvargal",
    "email": "maxsvargal@gmail.com",
    "url": "https://github.com/MaxSvargal"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MaxSvargal/kurrent/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "react-router",
    "flux",
    "webpack",
    "react-hot"
  ],
  "homepage": "https://github.com/MaxSvargal/kurrent#readme",
  "devDependencies": {
    "asar": "^0.12.3",
    "ava": "^0.17.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-tcomb": "^0.3.24",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-register": "^6.18.0",
    "babili-webpack-plugin": "^0.0.7",
    "boiler-room-custodian": "^0.4.2",
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "devtron": "^1.4.0",
    "electron": "^1.4.13",
    "electron-builder": "^10.17.3",
    "electron-devtools-installer": "^2.0.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.7.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-ava": "^4.0.1",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-flowtype-errors": "^2.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.14.0",
    "fbjs-scripts": "^0.7.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.37.0",
    "flow-typed": "^2.0.0",
    "html-webpack-plugin": "^2.24.1",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "nyc": "^10.0.0",
    "react-addons-test-utils": "^15.4.1",
    "redux-ava": "^2.2.0",
    "redux-logger": "^2.7.4",
    "redux-mock-store": "^1.2.1",
    "redux-saga": "^0.14.1",
    "redux-saga-test-plan": "^2.0.0",
    "sinon": "^1.17.7",
    "spectron": "^3.4.0",
    "tcomb": "^3.2.15",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-merge": "^1.0.2",
    "webpack-validator": "^2.2.11"
  },
  "dependencies": {
    "bittorrent-dht": "^7.4.0",
    "create-torrent": "^3.25.1",
    "drag-drop": "^2.12.1",
    "electron-debug": "^1.1.0",
    "ip": "^1.1.4",
    "kad": "^1.6.4",
    "magnet-uri": "^5.1.5",
    "parse-torrent": "^5.8.1",
    "quill": "^1.1.8",
    "radium": "^0.18.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-quill": "git+https://github.com/zenoamaro/react-quill.git",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "source-map-support": "^0.4.6",
    "stun-js": "^0.5.1",
    "webtorrent": "^0.98.0"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "**/*.test.js",
      "server/**/*",
      "app/data/*",
      "app/utils/*",
      "webpack.config.test.js"
    ],
    "extension": [
      ".jsx"
    ],
    "cache": true,
    "sourceMap": false,
    "instrument": false
  }
}
